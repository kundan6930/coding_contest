#include <bits/stdc++.h>
using namespace std;
using ll=long long;
int main (){
 ios_base::sync_with_stdio(false);
cin.tie(NULL);
ll n;
cin>>n;
vector<ll>a(n);
vector<vector<ll>>b(n+1);
for(ll i=0;i<n;i++)
{
    cin>>a[i];
    b[a[i]].push_back(i);
}
ll ans=0;
for(ll i=1;i<=n;i++)
{
   ll n1=b[i].size();
   ll s=0;
   for(ll j=0;j<n1-1;j++)
   {
      ll k1=j*b[i][j]-(n1-1-j)*b[i][j];
      ans+=k1;
   }
   cout<<ans<<" "<<i<<endl;
   ans-=((n*(n-1))/2);
cout<<ans<<" "<<i<<endl;
   for(ll j=1;j<=n1-2;j++ )
   {
       s+=j;
       ans-=s; 
   }
  cout<<ans<<" "<<i<<endl;
}
cout<<ans<<endl;
    return 0;
}